{
  "name": "google-sheets-table",
  "version": "0.2.0",
  "description": "Treat a Google Sheet like a database table",
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.2.4"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsup ./src/index.ts --format cjs,esm --dts && npx fix-tsup-cjs",
    "format": "prettier --write .",
    "test": "tap --ts --reporter=spec"
  },
  "prepublish": "tsc",
  "repository": {
    "type": "git",
    "url": "https://github.com/twistedstream/google-sheets-table.git"
  },
  "author": "twistedstream",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twistedstream/google-sheets-table/issues"
  },
  "homepage": "https://github.com/twistedstream/google-sheets-table#readme",
  "dependencies": {
    "@googleapis/drive": "^8.6.0",
    "@googleapis/sheets": "^5.0.5",
    "google-auth-library": "^9.5.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.5",
    "@types/sinon": "^17.0.3",
    "@types/tap": "^15.0.7",
    "fix-tsup-cjs": "^1.2.0",
    "prettier": "^3.2.4",
    "sinon": "^17.0.1",
    "tap": "^16.3.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "tap": {
    "coverage-map": "code-coverage-map.js"
  }
}
